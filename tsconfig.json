{
    "compilerOptions": {
        "esModuleInterop": true,
        "lib": ["ESNext"],
        "module": "ESNext",
        "moduleResolution": "Node",
        "noEmit": true,
        "resolveJsonModule": true,
        "skipLibCheck": true,
        "strict": true,
        "strictNullChecks": true,
        "target": "ES2020"
    }
}
