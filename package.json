{
    "name": "@awuxtron/utils",
    "type": "module",
    "version": "0.1.4",
    "description": "The collection of common Javascript / Typescript utilities",
    "keywords": [
        "utils"
    ],
    "homepage": "https://github.com/awuxtron/utils#readme",
    "bugs": {
        "email": "awuxtron@gmail.com",
        "url": "https://github.com/awuxtron/utils/issues"
    },
    "license": "MIT",
    "author": "Diep Dang <awuxtron@gmail.com> (https://awuxtron.dev)",
    "funding": "https://paypal.me/awuxtron",
    "files": [
        "dist"
    ],
    "main": "dist/index.cjs",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "require": "./dist/index.cjs",
            "import": "./dist/index.mjs"
        }
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/awuxtron/utils.git"
    },
    "scripts": {
        "build": "tsc && unbuild",
        "prepublishOnly": "npm run build",
        "release": "bumpp && npm publish"
    },
    "dependencies": {
        "deepmerge": "^4.3.1",
        "throttle-debounce": "^5.0.0"
    },
    "devDependencies": {
        "@types/node": "^18.16.3",
        "@types/throttle-debounce": "^5.0.0",
        "bumpp": "^9.1.0",
        "typescript": "^5.0.4",
        "unbuild": "^1.2.1"
    },
    "publishConfig": {
        "access": "public"
    }
}
